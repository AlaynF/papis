<div ng-controller="contact" class= "container">
    <label class="label">Name</label>
    <p class="control">
        <input ng-model="fullname" class="input is-success" type="text" placeholder="Text input" id="fullname">
    </p>
    <label class="label">Email</label>
    <p class="control has-icon has-icon-right">
        <input ng-model="email"  class="input is-success" type="text" placeholder="Email" id="email" >
    </p>
    <label class="label">Message</label>
    <p class="control">
        <textarea ng-model="message" class="textarea" placeholder="Message" id="message"></textarea>
    </p>
    <p class="control">
        <button ng-click="contactinfo();" class="button is-primary">Submit</button>
        <button ng-click="clear();" class="button is-link">Cancel</button>
    </p>
